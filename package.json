{
  "name": "reactive-kata",
  "version": "0.0.0",
  "description": "Reactive component kata",
  "author": "Matt Morgan",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matthewmorgan/reactive-kata.git"
  },
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^20.0.1",
    "jest-cli": "^20.0.4"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "package.json"
    ]
  },
  "scripts": {
    "test": "jest ./*",
    "watch": "jest --watch ./*",
    "watch:cover": "jest --coverage --watch ./*",
    "lint": "eslint .",
    "lint-test": "eslint . && jest --no-cache ./* "
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "extends": "standard",
    "rules": {
      "import/no-unresolved": "off",
      "import/extensions": "off"
    }
  },
  "licenses": [
    "MIT"
  ],
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/matthewmorgan/reactive-kata/issues"
  },
  "homepage": "https://github.com/matthewmorgan/reactive-kata#readme",
  "main": "index.js"
}
